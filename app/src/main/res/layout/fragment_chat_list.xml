<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="ui.chatList.ChatListFragment">

    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/list_chat_view"
        tools:listitem="@layout/fragment_chat_list_item"
        android:paddingBottom="150dp"
        android:clipToPadding="false"
        />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/sendDifu"
        android:layout_gravity="end|bottom"
        android:layout_margin="16dp"
        android:contentDescription="send difu"
        app:srcCompat="@drawable/ic_baseline_group_24"
        app:backgroundTint="@color/green_logo"/>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/serachChat"
        android:layout_gravity="end|bottom"
        android:layout_marginBottom="90dp"
        android:layout_marginEnd="16dp"
        android:contentDescription="seachatrch a "
        app:srcCompat="@drawable/ic_baseline_search_24"
        app:backgroundTint="@color/green_logo"/>

</FrameLayout>